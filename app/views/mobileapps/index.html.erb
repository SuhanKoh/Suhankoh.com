<!DOCTYPE html>
<html lang="en" ng-app="SuhanProjects">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
  <meta name="description" content="">
  <meta name="author" content="">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>

  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
  <link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.indigo-blue.min.css"/>
  <script defer src="https://code.getmdl.io/1.1.3/material.min.js"></script>
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" type="text/css">
  <title>Suhan</title>
  <!-- Bootstrap core CSS -->
  <%= stylesheet_link_tag "global.css" %>
  <%= stylesheet_link_tag "card.css" %>
  <%= stylesheet_link_tag "projects.css" %>
  <%= stylesheet_link_tag "bootstrap.min.css" %>
  <style>
    #view-source {
      position: fixed;
      display: block;
      right: 0;
      bottom: 0;
      margin-right: 40px;
      margin-bottom: 40px;
      z-index: 900;
    }
  </style>

</head>

<body>
<div class="layout-transparent mdl-layout mdl-js-layout">
  <header class="mdl-layout__header mdl-layout__header--transparent">
    <div class="mdl-layout__header-row">
      <!-- Title -->
      <span class="mdl-layout-title">Suhan</span>
      <!-- Add spacer, to align navigation to the right -->
      <div class="mdl-layout-spacer"></div>
      <!-- Navigation -->
      <nav class="mdl-navigation">
        <a class="mdl-navigation__link" href="./"><i class="material-icons">book</i> Résumé</a>
        <a class="mdl-navigation__link" href="./blog"><i class="material-icons">bookmark_border</i> Blog</a>
        <a class="mdl-navigation__link" href="https://www.linkedin.com/in/suhankoh"><i class="material-icons">link</i>
          LinkedIn</a>
        <a class="mdl-navigation__link" href="https://github.com/SuhanKoh"><i class="material-icons">link</i>
          GitHub</a>
        <a class="mdl-navigation__link link_tab" href="#privacypolicy"><i class="material-icons">link</i>
          Privacy Policy</a>
        <a class="mdl-navigation__link link_tab" href="#disclaimer"><i class="material-icons">link</i>
          Disclaimer</a>
        <a class="mdl-navigation__link link_tab" href="#termofuse"><i class="material-icons">link</i>
          Term of Use</a>
      </nav>
    </div>
  </header>
  <div class="mdl-layout__drawer">
    <span class="mdl-layout-title">Suhan</span>
    <hr>
    <nav class="mdl-navigation">
      <a class="mdl-navigation__link link_tab" href="./"><i class="material-icons">book</i> Résumé</a>
      <a class="mdl-navigation__link" href="./blog"><i class="material-icons">bookmark_border</i> Blog</a>
      <a class="mdl-navigation__link" href="https://www.linkedin.com/in/suhankoh"><i class="material-icons">link</i>
        LinkedIn</a>
      <a class="mdl-navigation__link" href="https://github.com/SuhanKoh"><i class="material-icons">link</i>GitHub</a>
      <a class="mdl-navigation__link link_tab" href="#privacypolicy"><i class="material-icons">link</i>
        Privacy Policy</a>
      <a class="mdl-navigation__link link_tab" href="#disclaimer"><i class="material-icons">link</i>
        Disclaimer</a>
      <a class="mdl-navigation__link link_tab" href="#termofuse"><i class="material-icons">link</i>
        Term of Use</a>
    </nav>
    <hr>
  </div>

  <main class="mdl-layout__content">
    <section class="mdl-layout__tab-panel is-active center resume-card-content" id="privacypolicy">
      <%= render :template => :'mobileapps/privacypolicy' %>
    </section>
    <section class="mdl-layout__tab-panel center resume-card-content" id="disclaimer">
      <%= render :template => :'mobileapps/disclaimer' %>
    </section>
    <section class="mdl-layout__tab-panel center resume-card-content" id="termofuse">
      <%= render :template => :'mobileapps/termofuse' %>
    </section>
  </main>
</div>

<%= javascript_include_tag "application", "data-turbolinks-track" => true %>
<script>
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r;
        i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
        a = s.createElement(o),
            m = s.getElementsByTagName(o)[0];
        a.async = 1;
        a.src = g;
        m.parentNode.insertBefore(a, m)
    })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
    ga('create', 'UA-71609259-1', 'auto');
    ga('send', 'pageview');
</script>
</body>
</html>
